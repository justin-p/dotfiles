[core]
	autocrlf = false
    branch = auto
    diff = auto
    interactive = auto
    pager = true
    status = auto
    ui = true
	editor = code -n -w # VS Code	
	excludesfile = ~/.gitignore_global
	
[color]
	ui = true	
	
[push]
	default = current	

[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
